<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

  <display-name>servlet-basic-dir</display-name>

  <!-- Start of Servlet declaration -->
  <servlet>
    <servlet-name>Welcome</servlet-name>
    <servlet-class>Welcome</servlet-class>
  </servlet>

  <servlet>
  <servlet-name>ServEx_2</servlet-name>
    <servlet-class>ServEx_2</servlet-class>
  </servlet>

  <servlet>
  <servlet-name>ServEx_2_Handler</servlet-name>
    <servlet-class>ServEx_2_Handler</servlet-class>
  </servlet>


  <servlet>
  <servlet-name>ServEx_3</servlet-name>
    <servlet-class>ServEx_3</servlet-class>
  </servlet>

  <servlet>
  <servlet-name>ServEx_4</servlet-name>
    <servlet-class>ServEx_4</servlet-class>
  </servlet>

  <servlet>
  <servlet-name>ServEx_4_Handler</servlet-name>
    <servlet-class>ServEx_4_Handler</servlet-class>
  </servlet>

  <servlet>
  <servlet-name>ServEx_5</servlet-name>
    <servlet-class>ServEx_5</servlet-class>
  </servlet>

  <servlet>
  <servlet-name>ServEx_6</servlet-name>
    <servlet-class>ServEx_6</servlet-class>
  </servlet>

  <servlet>
  <servlet-name>ServEx_6_Handler</servlet-name>
    <servlet-class>ServEx_6_Handler</servlet-class>
  </servlet>

  <servlet>
  <servlet-name>ServEx_7</servlet-name>
    <servlet-class>ServEx_7</servlet-class>
  </servlet>

  <!-- Start of mapping -->
  <servlet-mapping>
    <servlet-name>Welcome</servlet-name>
    <url-pattern>/welcome</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>ServEx_2</servlet-name>
    <url-pattern>/servEx_2</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>ServEx_2_Handler</servlet-name>
    <url-pattern>/servEx_2/handling-form</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>ServEx_3</servlet-name>
    <url-pattern>/servEx_3</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>ServEx_4</servlet-name>
    <url-pattern>/servEx_4</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>ServEx_4_Handler</servlet-name>
    <url-pattern>/servEx_4/handling-form</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>ServEx_5</servlet-name>
    <url-pattern>/servEx_5</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>ServEx_6</servlet-name>
    <url-pattern>/servEx_6</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>ServEx_6_Handler</servlet-name>
    <url-pattern>/servEx_6/handling-form</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>ServEx_7</servlet-name>
    <url-pattern>/servEx_7</url-pattern>
  </servlet-mapping>

</web-app>